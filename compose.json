{
  "version": "2.1",
  "networks": {
    "default": {
      "ipam": {
        "config": [
          {
            "subnet": "172.28.130.0/23",
            "gateway": "172.28.130.1",
            "ip_range": "172.28.131.0/24"
          }
        ]
      }
    }
  },
  "services": {
    "admin": {
      "volumes": [
        "/data:/data",
        "/config:/config",
        "/var/run/docker.sock:/var/run/docker.sock"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns": "172.28.128.1",
      "dns_search": "node.consul",
      "image": "${BLOCK_REGISTRY}/block:admin",
      "hostname": "admin"
    },
    "consul": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}",
        "8400:8400"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns": "172.28.128.1",
      "dns_search": "node.consul",
      "image": "${BLOCK_REGISTRY}/block:consul",
      "hostname": "consul"
    },
    "dnsmasq": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}",
        "53:5353",
        "53:5353/udp"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns": "172.28.128.1",
      "dns_search": "node.consul",
      "image": "${BLOCK_REGISTRY}/block:dnsmasq",
      "hostname": "dnsmasq"
    },
    "openvpn": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}",
        "1194:1194"
      ],
      "privileged": true,
      "cap_add": [
        "NET_ADMIN"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns": "172.28.128.1",
      "dns_search": "node.consul",
      "image": "${BLOCK_REGISTRY}/block:openvpn",
      "hostname": "openvpn"
    },
    "squid": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}",
        "3128:3128"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns": "172.28.128.1",
      "dns_search": "node.consul",
      "image": "${BLOCK_REGISTRY}/block:squid",
      "hostname": "squid"
    },
    "nexus": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns": "172.28.128.1",
      "dns_search": "node.consul",
      "image": "${BLOCK_REGISTRY}/block:nexus",
      "hostname": "nexus"
    },
    "nginx": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}",
        "80:8888"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns": "172.28.128.1",
      "dns_search": "node.consul",
      "image": "${BLOCK_REGISTRY}/block:nginx",
      "hostname": "nginx"
    }
  }
}
